{
	"_version": "1.8.0",
	"sap.app": {
		"id": "mccoy.com.URE2",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"UreMetadata.xsodata": {
				"uri": "/UreRaceData/OData/UreMetadata.xsodata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "mccoy.com.URE2.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "mccoy.com.URE2.i18n.i18n"
				}
			},
			"UreMetadata": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "UreMetadata.xsodata",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "mccoy.com.URE2.view",
				"controlAggregation": "pages",
				"controlId": "URE2App"
			},
			"routes": [
				{
					"name":		"home",
					"pattern":	"",
					"target":	"startpage"
				},
				{
					"name":		"newrun",
					"pattern":	"newrun",
					"target":	"NewRun"
				},
				{
					"name":		"dashboard",
					"pattern":	"dashboard",
					"target":	"Dashboard"
				}
			],
			"targets": {
				"startpage": {
					"viewName":		"Startpage",
					"viewlevel":	0
				},
				"NewRun": {
					"viewName":		"NewRun",
					"viewLevel":	1
				},
				"Dashboard": {
					"viewName":		"Dashboard",
					"viewLevel":	1
				}
			}
		}
	}
}